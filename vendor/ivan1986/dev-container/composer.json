{
    "name": "ivan1986/dev-container",
    "description": "Create vm for develop by ansible and docker",
    "homepage": "https://github.com/ivan1986/dev-container",
    "keywords": ["docker", "ansible", "dev"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": { "Ivan1986\\DevContainer\\": "src" }
    },
    "authors": [
        {
            "name": "Ivan Borzenkov",
            "email": "ivan.borzenkov@gmail.com"
        }
    ],
    "bin": [
        "bin/container"
    ],
    "require": {
        "symfony/console": "~3.0",
        "symfony/event-dispatcher": "~3.0",
        "eloquent/composer-config-reader": "~2.0",
        "docker-php/docker-php": "~1.24"
    },
    "provide": {
        "ext-mbstring": "*"
    },
    "suggest": {
        "php-school/cli-menu": "For init menu"
    }
}
